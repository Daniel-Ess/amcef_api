{
	"name": "amcef_api",
	"main": "src/index.ts",
	"license": "MIT",
	"version": "0.0.1",
	"author": "Daniel Stehura <stehura.daniel@gmail.com>",
	"description": "ToDo lists management API",
	"scripts": {
		"start": "cross-env TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true node -r dotenv/config -r ts-node/register src/index.ts",
		"lint": "esw src tests --color --ext .js --ext .ts",
		"lint-fix": "esw src tests --color --ext .js --ext .ts --fix",
		"lint-watch": "npm run lint -- --watch  --ext .js --ext .ts",
		"apidoc-scan": "apidoc -i src/ -o apidoc/ -f \".{1,}(.apidoc)$\"",
		"debug:script": "cross-env NODE_ENV=development TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true nodemon --watch src -L --exec 'node --inspect -r dotenv/config -r ts-node/register src/index.ts'",
		"debug": "npm-run-all -p lint-watch debug:script",
		"test": "cross-env NODE_ENV=test API_KEY=test node --expose-gc \"./node_modules/jest/bin/jest.js\" --runInBand --passWithNoTests",
		"test:debug": "cross-env NODE_ENV=test API_KEY=test node --expose-gc --inspect=9999 \"./node_modules/jest/bin/jest.js\" --runInBand --passWithNoTests --no-cache --logHeapUsage --watch"
	},
	"dependencies": {
		"body-parser": "1.20.2",
		"config": "3.3.9",
		"cors": "2.8.5",
		"cross-env": "7.0.3",
		"dotenv": "16.0.3",
		"express": "4.18.2",
		"helmet": "6.1.5",
		"joi": "17.9.2",
		"lodash": "4.17.21",
		"util": "0.12.5",
		"winston": "3.8.2",
		"winston-daily-rotate-file": "4.7.1"
	},
	"devDependencies": {
		"@types/config": "3.3.0",
		"@types/cors": "2.8.13",
		"@types/express": "4.17.17",
		"@types/jest": "29.5.1",
		"@types/lodash": "4.14.194",
		"@types/node": "18.16.1",
		"@types/request": "2.48.8",
		"@types/supertest": "2.0.12",
		"@typescript-eslint/eslint-plugin": "5.59.1",
		"@typescript-eslint/parser": "5.59.1",
		"apidoc": "0.54.0",
		"eslint": "8.39.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-watch": "8.0.0",
		"jest": "29.5.0",
		"lint-staged": "13.2.2",
		"nodemon": "2.0.22",
		"npm-run-all": "4.1.5",
		"supertest": "6.3.3",
		"ts-jest": "29.1.0",
		"ts-node": "10.9.1",
		"typescript": "5.0.4"
	}
}
